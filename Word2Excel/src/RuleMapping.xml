<?xml version="1.0" encoding="UTF-8"?>

<projects > 

	<project  >		
		<group key="01-招标文件" >
	
			<th rule = "folder"  level="1" type="text" title="开标日期" ></th>
			
			<th rule = "content" content-type='paragraph|table' type="text" title="招标编号" ></th>
			
			<th rule = "content"  content-type='paragraph|table' type="text" title='项目公司' key='招标人' sukey="公司"></th>
			
			<th rule = "folder"  level="2" type="text" isImportant="ture" title="工程名+标段名+数量"></th>
			
			<th rule = "content"  type="text"  title="项目经理"  key='联系人'></th>

			<th rule = "content"  type="text"  title="项目地理坐标" key='地理位置为'  >
				<keyword> 地理坐标</keyword>
				<keyword> 地理位置</keyword>
			</th>
			
			<th rule = "content"  type="text"  title="海拔高度"  key='海拔是'>
				<keyword>海拔高度</keyword>
			</th>
			
			<th rule = "content"  type="text"  title="地形地貌"  key='地貌以'>
				<keyword>地貌以</keyword>
			</th>
			
			<th rule = "content"  type="text"  title="年均气温" key="平均气温是" ></th>
			<th rule = "content"  type="text"  title="平均风速" key="平均风速是"></th>
			<th rule = "content"  type="text"  title="平均风功率密度" key='平均风功率密度为' ></th>
			<th rule = "content"  type="text"  title="机组容量（MW）" key="总容量为" unit="MW"></th>
			<th rule = "content"  type="text"  title="机组配置"></th>
			<th rule = "content"  type="text"  title="机型要求" key="机型为"></th>
			<th rule = "content"  type="text"  title="机型的安全等级" key='安全等级是'></th>
			<th rule = "content"  type="text"  title="发电小时数" key="发电小时数是"></th>
			<th rule = "content"  type="text"  title="设计寿命" key="寿命是"></th>
			<th rule = "content"  type="text"  title="项目交货期" key='项目交货期为'></th>
		</group>
		
		<group key="02-投标文件" >
				<!-- 提取投标文件-*-价格中的投标人栏目下方表格-价格汇总表   -->
				<th rule = "content" type='number' title='投标报价（万元）' key="投标总价" content-type='table' direction="v" />
				<!-- 提取投标文件-*-价格中的投标人栏目 -->
				<th rule = "content" type="text" title="投标人名称" key="投标人" sukey="公司"/>
				
				<!-- 提取投标文件-*-价格中的投标人栏目下方表格投标总价 -->
				<th rule = "undefinition" type="text" title="投标风机型号" key="投标机型" content-type='table' direction="v"/>
				                                                                                                                                                          
				
				<!-- 提取投标文件-02-投标文件-设备技术性能要求 -->
				<th rule = "content" type="enumeration" title="机型形式" key="机型为">	
					<enumeration>双馈</enumeration>
					<enumeration>直驱</enumeration>
					<enumeration>半直驱</enumeration>
				</th>
				
				<!--提取投标文件-*-技术中的轮毂高度为特征词  -->
				<th rule = "content" type="number" title="轮毂高度（m）" key="轮毂高度" content-type="table"  direction="h" unit="m"/>
				
				<!--提取投标文件-*-价格中的投标人栏目下方表格-价格汇总表  -->
				<th rule = "content" type="number" title="风电主机单价（万元）" key="单价" content-type='table' direction="v" />
				
				<!-- 提取投标文件-*-价格中的投标人栏目下方表格-价格汇总表 -->
				<th rule = "content" type="number" title="主机数量" key="台数" content-type='table' direction="v" />
				
				<th rule = "content" type="number" title="风轮直径" content-type="table"  direction="h" unit="m"/>
				<th rule = "content" type="number" title="扫风面积" content-type="table"  direction="h" unit="m2"/>
				<th rule = "content" type="number" title="额定风速" content-type="table"  direction="h" unit="m/s"/>
				<th rule = "content" type="number" title="切入风速" content-type="table"  direction="h" unit="m/s"/>
				<th rule = "content" type="number" title="切出风速" content-type="table"  direction="h" unit="m/s"/>
				<th rule = "content" type="number" title="叶片长度" content-type="table"  direction="h" unit="m"/>
				
				<!--提取投标文件-*-技术中的无明显特征词  -->
				<th rule = "content" type="text" title="厂家备注" key="结论是">
					
				</th>
				
		</group>
	</project>
	
</projects>
